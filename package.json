{
  "name": "vite-template-solid",
  "version": "0.0.0",
  "description": "",
  "browserslist": [
    "since 2018 and not dead, node 16"
  ],
  "scripts": {
    "start": "vite",
    "dev:nossr": "vite --config vite.non_ssr.config.ts",
    "dev": "node server.mjs",
    "prod": "cross-env NODE_ENV=production node server.mjs",
    "build": "npm run build:client && npm run build:server",
    "build:server": "vite build --outDir dist/lib  --ssr src/index-entry-server.tsx ",
    "build:client": "vite build  --outDir dist/public   ",
    "serve": "vite preview",
    "rbuild:ssr": "rollup -c rollup.config.js"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@rollup/plugin-babel": "^5.3.1",
